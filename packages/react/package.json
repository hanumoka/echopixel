{
  "name": "@echopixel/react",
  "version": "0.0.1",
  "description": "React components for EchoPixel DICOM viewer",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
